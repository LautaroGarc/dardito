{
  "name": "dardito",
  "version": "1.0.0",
  "description": "Sistema de gestión ágil con bot de Discord y dashboard web",
  "main": "start.js",
  "scripts": {
    "start": "node start.js",
    "dev": "concurrently \"npm run dev:bot\" \"npm run dev:web\"",
    "dev:bot": "nodemon bot.js --watch bot.js --watch commands/ --watch functions.js --watch config.json",
    "dev:web": "nodemon app.js --watch app.js --watch views/ --watch config.json",
    "dev:start": "nodemon start.js --watch start.js --watch config.json",
    "bot": "node bot.js",
    "web": "node app.js",
    "deploy": "node deploy-commands.js"
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "discord.js": "^14.14.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10"
  },
  "keywords": [
    "discord",
    "bot",
    "dashboard",
    "agile",
    "scrum"
  ],
  "author": "Tu Nombre",
  "license": "MIT"
}
